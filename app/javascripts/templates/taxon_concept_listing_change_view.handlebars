<div class="listing-section">
  <div class="listing-section-header">
    <div class="appendix">CITES Appendix {{unbound species_listing_name}}</div>
    <div class="party">{{#if hasParty}}{{unbound party.name}}{{/if}}</div>
    <div class="effective_at">{{unbound effective_at}}</div>
  </div>
  <div class="text-frame">
    {{#if hasFullNote}}
    <p class="short-note">{{unbound full_note}}</p>
    {{else}}
      {{#if hasShortNote}}
    <p class="long-note">{{{unbound short_note}}}</p>
      {{/if}}
    {{/if}}
    {{#if hasHashFullNote}}
    <p class="short-note">{{unbound hash_ann_symbol}}{{unbound hash_ann_parent_symbol}}</p>
    <p class="long-note">{{{unbound hash_full_note}}}</p>
    {{/if}}
  </div>
</div>
