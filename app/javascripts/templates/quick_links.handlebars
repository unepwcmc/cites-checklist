<div class="dropdown-links-holder">
  <h3>{{t featured.title}}</h3>
  <ul class="dropdown-links">
    <li>
      <a class="no-link">{{t featured.index}}</a>
      <div class="drop">
        <h3>{{t featured.format}}</h3>

        <ul>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="index" data-format="pdf">PDF</a></li>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="index" data-format="csv">CSV</a></li>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="index" data-format="json">JSON</a></li>
        </ul>
        <img src="/images/decor-dropdown-dark.png" class="decor" width="24" height="13" >
      </div>
    </li>
    <li>
      <a class="no-link">{{t featured.history}}</a>
      <div class="drop">
        <h3>{{t featured.format}}</h3>

        <ul>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="history" data-format="pdf">PDF</a></li>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="history" data-format="csv">CSV</a></li>
          <li><a href="#" {{action "download" target="view" on="click"}} data-doc-type="history" data-format="json">JSON</a></li>
        </ul>
        <img src="/images/decor-dropdown-dark.png" class="decor" width="24" height="13" >
      </div>
    </li>
  </ul>
</div>
