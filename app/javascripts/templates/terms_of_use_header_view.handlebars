<div class="wrap">
  <div class="content">
    <div class="c1">

      <div class="logo-holder">
        <h1 class="logo"><a href="/">{{t title}}</a></h1>
        <a href="#" {{action "doTermsOfUse"}} class="top-links">{{t toplink.terms}}</a>
        <a href="#" {{action "doAbout"}} class="top-links">{{t toplink.about}}</a>
        <div class="locale-selector top-links">
          <a href="#" {{action doLocaleEN}}>{{t language.en}}</a>
          <a href="#" {{action doLocaleES}}>{{t language.es}}</a>
          <a href="#" {{action doLocaleFR}}>{{t language.fr}}</a>
        </div>
      </div>

      <div class="header-frame">
        <h1>{{t terms.title}}</h1>
      </div>

    </div>
  </div>

  <div class="aside">
  </div>
</div>
