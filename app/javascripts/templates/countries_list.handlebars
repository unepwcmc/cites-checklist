<div class="country-holder">
  {{view.shortText}}
  {{#if view.showMore}}
    and {{view.count}} more.
    <a class="btn-saved">
      See all...
      <div class="drop">
        <div class="drop-holder">
          <ul>
            {{#each view.fullText}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
        </div>
        <img src="/images/decor-dropdown-dark.png" class="decor" width="24" height="13" >
      </div>
    </a>
  {{/if}}
</div>
