<div class="country-holder">
  {{view.shortText}}
  <a class="btn-saved">
    {{#if view.showMore}}
      and {{view.count}} more...
    {{/if}}
    <div class="drop">
      <div class="drop-holder">
        <ul>
          {{#each view.fullText}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      <img src="images/img-decor3.png" alt="image description" class="decor" width="24" height="13" >
    </div>
  </a>
</div>
