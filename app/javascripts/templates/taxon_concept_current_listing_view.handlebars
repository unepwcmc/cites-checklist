
<ul class="icons">

  {{#each view.currentListingParts}}
    <li>

      <div class="circle {{unbound this.colour}}">
        {{unbound this.symbol}}
      </div>

    </li>
  {{/each}}
</ul>


<div style="display: none">
  <div class="lightbox current-listing" id="current-listing-popup{{unbound id}}">
    <div class="heading-holder">
      <strong class="heading">{{t timeline.event.appendix appendixBinding="current_listing"}}<span class="parties">{{unbound view.current_parties}}</span></strong>
    </div>
    <div class="text-holder">
      {{collection Checklist.TaxonConceptCurrentListingChangesListView contentBinding="current_additions"}}
    </div>
  </div>
</div>
